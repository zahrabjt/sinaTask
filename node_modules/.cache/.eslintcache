ersist/lib/storage'\nimport { configure } from '@testing-library/react';\n\n\nconst persistConfig = {\n    key: 'root',\n    storage,\n  } \n\n\n  const persistedReducer = persistReducer(persistConfig, reducers)\n\n \nexport const store = createStore(persistedReducer , applyMiddleware(thunk));\nexport const persistor = persistStore(store);\n\n","/home/zahra/redux/src/stateManagement/reducers/index.jsx",[],[],"/home/zahra/redux/src/stateManagement/reducers/productReducer.jsx",[],[],"/home/zahra/redux/src/stateManagement/actions/actionType.jsx",[],[],"/home/zahra/redux/src/components/reduxList.jsx",["82","83"],[],"import React, { Component } from 'react'\nimport { getAll,remove } from '../stateManagement/actions/productActions'\nimport { connect } from 'react-redux'\n\n\nconst reduxList = (props) => {\n  return (\n    <div>\n      <h1>reduxList</h1>\n      <table className=\"table table-striped table-bordered\">\n        <thead>\n          <tr>\n            <th>Id</th>\n            <th>reduxName</th>\n            <th>Price</th>\n          </tr>\n        </thead>\n        <tbody>\n          {props.products.map((item) => (\n            <tr key={item.id}>\n              <td>{item.id}</td>\n              <td>{item.reduxName}</td>\n              <td>{item.price}</td>\n              <td>\n                <button className=\"btn btn-danger btn-sm\" onClick={() => props.removeRedux(item.id)}>Delete</button>\n              </td>\n            </tr>\n          ))}\n\n        </tbody>\n      </table>\n    </div>\n  );\n};\n\n\n\n\n//mapStateToProps => وصل کردن استیت داخل استور به یک پراپرتی  و خواندن اطلاعات\nconst mapStateToProps = (state) => ({\n  products: state.productState.items,\n});\n\n\n//برای نوشتن و تغییر در ریداکس\nconst mapDispatchToProps = dispatch => {\n  return{\n    removeRedux: (id) => dispatch(remove(id))\n  }\n}\n\nexport default connect(mapStateToProps,mapDispatchToProps)(reduxList);\n","/home/zahra/redux/src/stateManagement/actions/productActions.jsx",[],[],"